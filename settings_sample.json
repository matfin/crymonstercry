{
	"app": {
		"debug": true,
		"pollInterval": 60000
	},
	"contentful": {
		"endpointUrl": 			"{contentful_endpoint_in_here}",
		"authorisationHeader": 	"Bearer {your_auth_access_token}",
		"contentTypeHeader": 	"application/vnd.contentful.delivery.v1+json",
		"spaceId": 				"{your_contentful_space_id_here}",
		"key":					"Basic {your_auth_key_for_hooks}",
		"contentTypes": [
			{
				"name": "release",
				"id": "{content_type_id}"
			},
			{
				"name": "gig",
				"id": "{content_type_id}"
			},
			{
				"name": "page",
				"id": "{content_type_id}"
			},
			{
				"name": "video",
				"id": "{content_type_id}"
			}
		]
	},
	"tumblr": {
		"blogName":	"{your_blog_name}",
		"blogUrl": 		"{yourblog}.tumblr.com",
		"consumerKey": 	"{your_consumer_key}",
		"apiUrl": 		"api.tumblr.com",
		"filterParams": {
			"posts": {
				"filter": 	"html",
				"limit": 	12
			}
		}
	},
	"instagram": {
		"apiUrl": 			"https://api.instagram.com",
		"clientID": 		"{your_client_id}",
		"userId": 			"{your_user_id}"
	},
	"mailchimp": {
		"endpointUrl": "{your_mailchimp_endpoint_url}",
		"apiKey": "{your_api_key}",
		"listId": "{your_list_id}"
	}
}