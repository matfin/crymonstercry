{
	"app": {
		"debug": true,
		"pollInterval": 60000
	},
	"contentful": {
		"endpointUrl": 			"{contentful_endpoint_in_here}",
		"authorisationHeader": 	"Bearer {your_auth_access_token}",
		"contentTypeHeader": 	"application/vnd.contentful.delivery.v1+json",
		"spaceId": 				"{your_contentful_space_id_here}",
		"contentTypes": [
			{
				"name": "release",
				"id": "{content_type_id}"
			},
			{
				"name": "gig",
				"id": "{content_type_id}"
			},
			{
				"name": "page",
				"id": "{content_type_id}"
			}
		]
	},
	"youtube": {
		"endpointUrl": 	"https://www.googleapis.com/youtube/v3",
		"apiKey": 		"{your_youtube_api_key_here}",
		"channelUser": 	"{your_channel_user_here}"
	},
	"tumblr": {
		"blogUrl": 		"{yourblog}.tumblr.com",
		"consumerKey": 	"{your_consumer_key}",
		"apiUrl": 		"api.tumblr.com",
		"filterParams": {
			"posts": {
				"type": 	"text",
				"filter": 	"html"
			}
		}
	},
	"instagram": {
		"apiUrl": 			"https://api.instagram.com",
		"clientID": 		"{your_client_id}",
		"userId": 			"{your_user_id}"
	}
}